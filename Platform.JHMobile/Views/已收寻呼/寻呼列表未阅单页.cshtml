@using Platform.JHMobile.Models
@model List<f______列表寻呼未阅表_Result>
@{
    foreach (var obj in Model)
    {
        <div class="row" onclick="top.location = '@Url.Action("寻呼列表未阅内容", "已收寻呼", new { id = obj.CallNoSeeID })';">
            <div class="col-md-12">
                <div class="panel metro_0">
                    <div class="panel-heading" style="color:#fff;">
                        <div class="text-left" style="width: 40%; float: left;"><i class="fa fa-comments"></i><span>&nbsp;&nbsp;&nbsp;&nbsp;</span>@obj.UserName</div>
                        <div class="text-right" style="width: 40%; float: right;">@obj.CallTime.ToString("yyyy-MM-dd HH:mm:ss")</div>
                    </div>
                    <div style="clear: both;"></div>
                    <div class="panel-footer" style="margin-top: 6px;">
                        <div class="row">
                            <div class="col-md-12">@OfficeController.Cut(@Html.Raw(obj.CallContent).ToString(), int.MaxValue)</div>
                        </div>
                        <div class="clearfix"></div>
                    </div>
                </div>
            </div>
        </div>
    }
}
<div class="row">
    <div class="col-md-12 text-center">
        -&nbsp;&nbsp;&nbsp;&nbsp;第&nbsp;@(ViewBag.Current)&nbsp;页&nbsp;&nbsp;&nbsp;&nbsp;-
    </div>
</div>
<div class="row">&nbsp;</div>
